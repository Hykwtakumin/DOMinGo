{
  "name": "DOMinGo",
  "version": "0.0.1",
  "description": "Chrome extension starter pack with typescript",
  "icons": {
    "128": "icons/bigicon.png"
  },
  "browser_action": {},
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "tabCapture",
    "downloads",
    "contextMenus",
    "notifications",
    "<all_urls>"
  ],
  "background": {
    "script": "background.js",
    "page": "pages/background.html",
    "persistent": false
  },
  "options_page": "pages/option.html",
  "options_ui": {
    "page": "pages/option.html",
    "open_in_tab": true
  },
  "web_accessible_resources": ["/icons/bigicon.png"],
  "manifest_version": 2
}
